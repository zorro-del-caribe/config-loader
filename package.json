{
  "name": "conf-load",
  "version": "2.0.0",
  "description": "go through a config folder to namespace configuration values and load values based on NODE_ENV",
  "main": "index",
  "scripts": {
    "test": "NODE_ENV=test node ./test/index.js",
    "test:ci": "NODE_ENV=test node ./test/index.js | tap-set-exit",
    "build": "babel ./index.mjs --out-dir ."
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/zorro-del-caribe/config-loader.git"
  },
