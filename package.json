    "loader",
    "environment"
  ],
  "engines": {
    "node": ">=8.0.0"
  },
  "author": "Laurent Renard",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-dynamic-import-node": "^1.2.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "tap-set-exit": "^1.1.1",
    "zora": "^2.0.1"
  }
}
